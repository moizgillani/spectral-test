    "/detect": {
      "post": {
        "tags": [
          "Test"
        ],
        "summary": "Detect",
        "description": "Detects the language of text within a request.",
        "operationId": "Detect",
        "parameters": [
          {
            "name": "Content-Type",
            "in": "header",
            "description": "defining content type as x-www-form-urlencoded",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string",
              "example": "application/x-www-form-urlencoded"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "encoding": {},
              "schema": {
                "required": [
                  "q"
                ],
                "type": "object",
                "properties": {
                  "q": {
                    "type": "string",
                    "description": "The input text upon which to perform language detection.",
                    "example": "English is hard, but detectably so"
                  }
                }
              }
            }
          },
          "required": false
        },
        "responses": {
          "200": {
            "description": "",
            "headers": {},
            "content": {
              "text/plain": {
                "schema": {
                  "type": "object",
                  "example": {
                    "data": {
                      "detections": [
                        [
                          {
                            "confidence": 1,
                            "isReliable": false,
                            "language": "en"
                          }
                        ]
                      ]
                    }
                  }
                },
                "example": {
                  "data": {
                    "detections": [
                      [
                        {
                          "confidence": 1,
                          "isReliable": false,
